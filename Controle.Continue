package controle;

public class Continue {//objeto
public static void main(String[] args) {//iniciando programa
	for(int i =0;i<10;i++) {//for(;boolean ; ) repetição 
		if(i % 2==1) {//if(booblean) executa quando for verdadeiro 
			continue;// interrompe o bloco, mas continua a função, não sair da função 
		}
		System.out.println(i);//mostrando na tela 
		
	}
}
}

