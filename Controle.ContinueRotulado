package controle;

public class ContinueRotulado {// objeto
	public static void main(String[] args) {//inicio 
		externo://
			for (int i = 0; i <3 ; i++) {//função for 
			 for (int j = 0; j < 3; j++) {//função for 
			if(i==1) {//if 
				
				continue externo;// usando para chamar o for externo 
			}
			System.out.printf("[%d %d]", i,j);//mostrando 
			}
		System.out.println();//mostrando na tela 
			}
	System.out.println("Fim");//mostrando na tela 
	}
	}

