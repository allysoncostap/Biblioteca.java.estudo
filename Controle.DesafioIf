package controle;

public class DesafioIf {//objeto
	public static void main(String[] args) {// iniciando programa
		
		
		double nota = 1.3 ;//declarando variável 
		if(nota >=9.0);{// if( boolean) executa quando for verdadeiro 
			System.out.println("Quadro de honra");//mostrando na tela
			System.out.println("Voce é fera!!!");//mostrando na tela 
		}
	}
}

