package fundamentos;

public class Unarios {
	public static void main(String[] args ){
		   
		   int a = 1;//declarando variavel 
		   int b = 2;//declarando variavel
		   
		   
		   a++; // a = a +1//agrementando +1
		   a-- ;//a = a  - 1// degrementando -1
		   ++b;//b =b +1;//agrementrando com prioriedade 
		   --b;//b = b - 1;//degrementrando com prioriedade

		System.out.println(a);///mostrando na tela 
		System.out.println(b);///mostrando na tela 

		System.out.println("Mini Desafio... ");///mostrando na tela 
		System.out.println(++a == b--);///mostrando na tela + condi√ßoes 
		System.out.println(a);///mostrando na tela
		System.out.println(b);//mostrando na tela 

}
}
